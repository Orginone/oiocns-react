{
  "name": "OIOCNS-REACT",
  "version": "0.0.1",
  "description": "Vite + Typescript + React + Antd + Less + Eslint + Prettier",
  "keywords": [
    "OIOCNS-REACT",
    "奥集能"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "npm run dev",
    "dev": "vite --mode development",
    "build": "vite build",
    "build:beta": "vite build --mode beta",
    "build:release": "vite build --mode release",
    "build:legacy": "vite build --mode legacy ",
    "build:deploy": "vite build --base=/vite-react-ts/",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx  ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:prettier": "prettier -c --write \"src/**/*.tsx\" --end-of-line auto",
    "deploy": "gh-pages -d dist",
    "postinstall": "husky install",
    "commitlint": "commitlint",
    "preview": "vite preview --open",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "./src/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ],
    "*.{less,css}": [
      "prettier --parser --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-components": "^2.3.37",
    "@ant-design/pro-layout": "^7.1.4",
    "@icongo/im": "^1.1.2",
    "@microsoft/signalr": "^6.0.10",
    "@msgpack/msgpack": "^2.8.0",
    "@tanstack/react-query": "^4.13.0",
    "@tanstack/react-query-devtools": "^4.12.0",
    "@types/qrcode": "^1.5.0",
    "@wcjiang/notify": "^2.1.0",
    "antd": "^4.24.4",
    "antd-img-crop": "^4.2.8",
    "array-move": "^4.0.0",
    "axios": "^1.1.2",
    "copy-to-clipboard": "^3.3.2",
    "dayjs": "^1.10.6",
    "mockjs": "^1.1.0",
    "pako": "^2.0.4",
    "qrcode": "^1.5.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-sortable-hoc": "^2.0.0",
    "terser": "^5.15.1",
    "yarn": "^1.22.19",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/babel__core": "^7.1.14",
    "@types/node": "^18.8.4",
    "@types/react": "^18.0.21",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-legacy": "^2.3.1",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.3.1",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.3.6",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.2.3",
    "vite": "^3.1.7",
    "vite-aliases": "^0.9.2",
    "vite-plugin-babel-import": "^2.0.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imp": "^2.0.7",
    "vite-plugin-mock": "^2.9.4",
    "vite-plugin-style-import": "^2.0.0"
  }
}
