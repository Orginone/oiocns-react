{
  "name": "orginone",
  "version": "0.0.1",
  "description": "Vite + Typescript + React + Antd + Less + Eslint + Prettier",
  "keywords": [
    "orginone",
    "资产共享云"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "npm run dev",
    "dev": "vite --mode development",
    "build": "node --max_old_space_size=10240 ./node_modules/vite/bin/vite.js build",
    "build:beta": "vite build --mode beta",
    "build:release": "vite build --mode release",
    "build:legacy": "vite build --mode legacy ",
    "build:deploy": "vite build --base=/vite-react-ts/",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx  ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:prettier": "prettier -c --write \"src/**/*.tsx\" --end-of-line auto",
    "deploy": "gh-pages -d dist",
    "postinstall": "husky install",
    "commitlint": "commitlint",
    "preview": "vite preview --open",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "./src/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ],
    "*.{less,css}": [
      "prettier --parser --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-components": "^2.3.37",
    "@antv/x6": "^2.13.1",
    "@antv/x6-plugin-dnd": "^2.0.6",
    "@antv/x6-plugin-selection": "^2.1.7",
    "@antv/x6-react-shape": "^2.2.2",
    "@dnd-kit/core": "^6.0.7",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@hello-pangea/color-picker": "^3.2.2",
    "@microsoft/signalr": "^6.0.10",
    "@msgpack/msgpack": "^2.8.0",
    "@preact/signals-react": "^1.3.6",
    "@react-icons/all-files": "^4.1.0",
    "@types/crypto-js": "^4.1.2",
    "@types/react-scroll": "^1.8.7",
    "@uiw/codemirror-extensions-langs": "^4.21.3",
    "@uiw/codemirror-theme-vscode": "^4.21.3",
    "@uiw/react-codemirror": "^4.21.7",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-react": "^1.0.6",
    "antd": "^4.24.4",
    "axios": "^1.1.2",
    "codemirror": "^6.0.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.6",
    "devextreme": "^23.1.5",
    "devextreme-react": "^23.1.5",
    "echarts": "^5.4.2",
    "emoji-mart": "^5.5.2",
    "for-editor": "^0.3.5",
    "fr-generator": "^2.8.9",
    "html2canvas": "^1.4.1",
    "jol-player": "^4.0.0",
    "pako": "^2.0.4",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-cropper": "^2.3.3",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-office-viewer": "^1.0.4",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.8.9",
    "terser": "^5.15.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/babel__core": "^7.1.14",
    "@types/lodash": "^4.14.198",
    "@types/node": "^18.8.4",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^18.0.21",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-legacy": "^2.3.1",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.3.1",
    "conventional-changelog-cli": "^2.1.1",
    "csstype": "^3.1.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "postcss": "^8.3.6",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.2.3",
    "vite": "^3.1.7",
    "vite-aliases": "^0.9.2",
    "vite-plugin-babel-import": "^2.0.2",
    "vite-plugin-compression": "^0.5.1",
